[
  {
    "httpRequest": {
      "method": "GET",
      "path": "/opera/v1/guests",
      "queryStringParameters": {
        "room": [ { "exact": "203" } ],
        "lastName": [ { "regex": "(?i)Nguyen" } ]
      }
    },
    "httpResponse": {
      "statusCode": 200,
      "headers": { "Content-Type": [ "application/json" ] },
      "body": {
        "reservationId": "RES-555",
        "folioWindow": 1,
        "guestName": "Taylor Nguyen",
        "roomNumber": "203",
        "inHouse": true
      }
    }
  },
  {
    "httpRequest": { "path": "/opera/v1/guests" },
    "httpForward": { "host": "opera-state", "port": 5000, "scheme": "HTTP" }
  },
  {
    "httpRequest": { "path": { "regex": "/opera/v1/folios/.*" } },
    "httpForward": { "host": "opera-state", "port": 5000, "scheme": "HTTP" }
  }
]
